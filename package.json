{
  "name": "jazasoft",
  "version": "1.0.2",
  "description": "Jaza Soft component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/zahidraza/jazasoft.git"
  },
  "author": "Md Zahid Raza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zahidraza/jazasoft/issues"
  },
  "homepage": "https://github.com/zahidraza/jazasoft",
  "keywords": [
    "react-component",
    "react"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./lib",
    "build": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./lib",
    "build-watch": "babel --watch --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./lib",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "grommet": "^1.6.0",
    "grommet-addons": "^0.4.0",
    "immutable": "^3.8.2",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "moment": "^2.18.1",
    "nodemon": "^1.9.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "grommet": "^1.6.0",
    "grommet-addons": "^0.4.0",
    "immutable": "^3.8.2",
    "moment": "^2.18.1",
    "react": "^0.14.9 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.9 || ^15.0.0 || ^16.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.6.1",
    "history": "^4.6.3",
    "lodash": "^4.17.11",
    "prop-types": "^15.5.10",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-toolbox": "^2.0.0-beta.12",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0"
  }
}
